<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.petproject.controllers.MainWindowController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
   <AnchorPane prefHeight="200.0" prefWidth="200.0">
      <children>
         <CheckBox layoutX="449.0" layoutY="43.0" mnemonicParsing="false" onAction="#onTestCheckBoxClick" text="CheckBox" />
         <Label fx:id="testLabel" layoutX="16.0" layoutY="36.0" prefHeight="32.0" prefWidth="411.0" text="Label" />
         <Button layoutX="254.0" layoutY="88.0" mnemonicParsing="false" onAction="#onGoToLabButtonClick" text="go to lab" />
      </children>
   </AnchorPane>

  <Label fx:id="welcomeText" />
  <Button onAction="#onHelloButtonClick" text="Hello!" />
</VBox>
